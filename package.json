{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "yarn test:lint && yarn test:flow && yarn test:unit",
    "test:unit": "jest --maxWorkers=2",
    "test:lint": "eslint . --quiet",
    "test:watch": "jest --watch",
    "test:flow": "yarn flow",
    "test:flow:coverage": "flow-coverage-report --threshold=75 -i 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "@types/recompose": "^0.30.6",
    "@types/yup": "^0.26.22",
    "art": "^0.10.3",
    "axios": "^0.18.0",
    "d3": "^5.12.0",
    "d3-shape": "^1.3.5",
    "formik": "^1.5.8",
    "moment": "^2.29.2",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-formik": "^1.7.7",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-material-textfield": "^0.12.0",
    "react-native-root-toast": "^3.1.2",
    "react-native-safe-area-view": "^0.14.5",
    "react-native-svg": "^9.11.1",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^2.12.1",
    "recompose": "^0.30.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^23.3.2",
    "@types/react-test-renderer": "^16.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^5",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.1.0",
    "eslint-config-bambi": "^1.4.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.14.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.8.6",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "bambi"
  }
}
